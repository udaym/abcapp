<h1>Listing abcaps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abcaps.each do |abcap| %>
      <tr>
        <td><%= abcap.name %></td>
        <td><%= abcap.address %></td>
        <td><%= link_to 'Show', abcap %></td>
        <td><%= link_to 'Edit', edit_abcap_path(abcap) %></td>
        <td><%= link_to 'Destroy', abcap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Abcap', new_abcap_path %>
